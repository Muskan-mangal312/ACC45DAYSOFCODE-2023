#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T; // Number of test cases

    while (T--) {
        int N;
        cin >> N; // Number of friends

        // Calculate the minimum number of cars required
        int cars = (N + 3) / 4; // Round up to the nearest multiple of 4

        cout << cars << endl;
    }

    return 0;
}
