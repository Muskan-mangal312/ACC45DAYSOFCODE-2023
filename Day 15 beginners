#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T; // Number of test cases

    while (T--) {
        int N;
        cin >> N; // Seat number

        // Determine the type of seat based on seat number and deck
        if (N <= 15) {
            if (N % 2 == 0) {
                cout << "Lower Double" << endl;
            } else {
                cout << "Lower Single" << endl;
            }
        } else {
            if (N % 2 == 0) {
                cout << "Upper Double" << endl;
            } else {
                cout << "Upper Single" << endl;
            }
        }
    }

    return 0;
}
